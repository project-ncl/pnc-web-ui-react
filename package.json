{
  "name": "pnc-web-ui-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@patternfly/react-core": "^4.135.0",
    "@patternfly/react-table": "^4.29.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.3.2",
    "web-vitals": "^1.1.2",
    "pnc-api-types-ts": "project-ncl/pnc-api-types-ts#semver:^2.1.0"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version FAST_REFRESH=false react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "prettier-write": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prettier-check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.4",
    "@storybook/addon-essentials": "^6.4.4",
    "@storybook/addon-links": "^6.4.4",
    "@storybook/node-logger": "^6.4.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.4",
    "@types/react-test-renderer": "^17.0.1",
    "axios-mock-adapter": "^1.20.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --check --ignore-unknown"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
